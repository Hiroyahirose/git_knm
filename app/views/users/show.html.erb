<div class="contents">
    <div class="owner">
        <%= @nickname %>さんの投稿一覧
    </div>
    <% @posts.each do |post| %>
        <%= render partial: "posts/post", locals: { post: post } %>
    <% end %>
    <%= paginate(@posts) %>
    <% if user_signed_in? %>
       <%= render partial: "posts/button" %>
    <% end %>
</div>